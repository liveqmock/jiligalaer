.buttonBg_1{
	font-size:15px; 
	font-weight:800;
	font-family:微软雅黑; 
	color:#715201;
	background: url(../images/btns.png);
	width:101px; 
	height:29px;
	border:0px;
	cursor: pointer;
}
.buttonBg_2{
	font-size:14px; 
	font-weight: bold; 
	font-family:微软雅黑; 
	background: url(../images/btnc.png);
	width:101px; 
	height:30px;
	border:0px;
	cursor: pointer;
}
.select_but {
	background: url(../images/list.gif) no-repeat right #fff; 
	cursor:pointer;
	border:1px;
	border-bottom-color:#ABADB3;
	border-left-color:#ABADB3;
	height: 18px;
	outline: 1px solid #ABADB3 !important;
}

#div_view{
	display:none;
	background-color :#f6f7f7;
	color:#333333;
	font-size:12px;
	line-height:18px;
	border:1px solid #e1e3e2;
	width:150px;
	height:50px;
	padding: 5px;
}