<style type="text/css">
body {
	margin: 0px auto; background-color : #F2F2F2; background-repeat : repeat;
	background-position : left;
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	background-color: #F2F2F2;
	background-repeat: repeat;
	background-position: left;
}
</style>
<script type="text/javascript">
	function next() {
		var mdmTemplates =  document.getElementsByName("mdmTemplate");
		var mdmTemplateId = "";
		var tag = false;
		for ( var i = 0; i < mdmTemplates.length; i++) {
			if (mdmTemplates.item(i).checked) {
				tag = true;
				mdmTemplateId = mdmTemplateId + mdmTemplates.item(i).getAttribute("value");
				continue;
			}
		}
		if(mdmTemplateId == ''){
			alert("请选择模板!");
			return ;
		}
		var form = document.forms.listForm;
		form.action = "${ctx}/combinProd/computResource.do?mdmTemplateId="+mdmTemplateId;
		form.submit();
	}
	function previous() {
		var form = document.forms.listForm;
		form.action = "${ctx}/combinProd/selectDc.do";
		form.submit();
	}
</script>

</head>
