}

.btn_query  {
	cursor: pointer;
	width:24px;
	height:24px;
	background:url(../images/ico14.png);
	background-repeat: no-repeat;
}

.casing {}
.casing .caput { height: 12px; font-size: 1px; }
.caput span, .trail span { width: 20px; height: 12px; font-size: 1px; }
.caput .left {  float: left; }
.caput .right { float: right; }
.casing .viscera {  margin-right: 10px;   overflow: hidden; }
.sheet_div { overflow: auto;width:100%; height: 100%;margin-top:-3px;}

.searchbox { font-size:12px; background:#ffffff; padding-top:5px; top: expression(this.offsetParent.scrollTop);left: expression(this.parentElement.scrollLeft);z-index:100;   _width:100%;white-space: nowrap;}
.searchbox table{ width:100%;border-collapse:collapse;}
.searchbox tr td input, .searchbox tr td select { border: 1px solid #93ACC4; width: 100px; padding:1px;font-family:"微软雅黑";}
.searchbox tr td,.searchbox tr th{padding:0 3px 0 5px ; line-height:28px; white-space:nowrap; font-weight:normal;}
.searchbox tr th{ text-align:right}


