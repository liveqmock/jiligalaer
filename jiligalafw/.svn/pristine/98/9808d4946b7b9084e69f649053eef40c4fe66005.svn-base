package sy.domain.vo.base;

import java.util.Date;

import sy.common.util.valuesetter.Create;
import sy.common.util.valuesetter.Update;
import sy.domain.vo.shared.BaseVo;

/**
 *
 * @author xhchen
 *
 */
public class BasicUserActivationCodeVo  extends BaseVo{

	/**
	 * 
	 */
	private static final long serialVersionUID = -2643130688211913761L;

	private String activationCode;
	private String userId;
	private Integer state;
	
	@Create
	private Date created;
	
	@Update
	private Date updated;
	

	public String getActivationCode() {
		return activationCode;
	}

	public String getUserId() {
		return userId;
	}

	public Integer getState() {
		return state;
	}

	public Date getCreated() {
		return created;
	}

	public Date getUpdated() {
		return updated;
	}

	public void setActivationCode(String activationCode) {
		this.activationCode = activationCode;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public void setState(Integer state) {
		this.state = state;
	}

	public void setCreated(Date created) {
		this.created = created;
	}

	public void setUpdated(Date updated) {
		this.updated = updated;
	}

	

	
	
}
